{
    "name": "deb/flexibee-bricks",
    "description": "php-flexibee extending classes and widgets (debianized)",
    "homepage": "https://github.com/VitexSoftware/FlexiPeeHP-Bricks",
    "license": "GPL-2.0+",
    "version": "0.25",
    "type": "library",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "vitex@arachne.cz"
        }
    ],
    "require": {
        "deb/flexibee": "*",
        "deb/ease-bricks": "*"
    },
    "autoload": {
        "psr-4": {
            "FlexiPeeHP\\Bricks\\": "./Bricks",
            "FlexiPeeHP\\Bricks\\ConvertRules\\": "./Bricks/ConvertRules",
            "FlexiPeeHP\\ui\\": "./ui"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/Ease",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/EaseHtml",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/EaseTWB",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/EaseBricks",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/flexibee",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/FlexiBee",
            "options": {
                "symlink": true
            }
        }
    ]
}
